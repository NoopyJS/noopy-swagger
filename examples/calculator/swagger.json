{
  "openapi": "3.0.0",
  "info": {
    "title": "API Exemple",
    "version": "1.0.0",
    "description": "Documentation de l'API"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "summary": "Hello World",
        "responses": {
          "200": {
            "description": "Hello World",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/calculator": {
      "get": {
        "summary": "Returns a message from the calculator",
        "responses": {
          "200": {
            "description": "A successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
